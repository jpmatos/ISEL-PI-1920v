{{#if series}}
<table id="divSortTable" class="table text-center">
    <thead class="thead-dark">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Popularity</th>
            <th>Vote Average</th>
        </tr>
    </thead>
    <tbody>
        {{#each series}}
        <tr>
            <td><a href="https://www.themoviedb.org/tv/{{id}}"></a>{{id}}</td>
            <td>{{name}}</td>
            <td>{{popularity}}</td>
            <td>{{vote_average}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{else}}
<div id="divSortTable" class="card text-center bg-warning">
    <p class="card-text ">No series found</p>
</div>
{{/if}}