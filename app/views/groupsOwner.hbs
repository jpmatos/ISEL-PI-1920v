{{#if groupsOwner}}
<table class="table text-center">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Description</th>
            <th scope="col">Number of Series</th>
        </tr>
    </thead>
    <tbody id="groupsOwnerBody">
        {{#each groupsOwner}}
        <tr>
            <td><a href="#group/{{_id}}">{{name}}</a></td>
            <td>{{description}}</td>
            <td>{{series.length}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{else}}
<div id="noGroupsFoundCard" class="card text-center bg-warning">
    <p class="card-text ">No groups found</p>
</div>
{{/if}}